<%= form_for :session, :url => session_path do |form| %>
  <div class="clearfix">
    <%= form.label :email %>
    <div class="input">
    <%= form.text_field :email, :type => "email" %>
    </div>
  </div>
  <div class="clearfix">
    <%= form.label :password %>
    <div class="input">
    <%= form.password_field :password %>
    </div>
  </div>
  <div class="actions">
    <%= form.submit "Войти", :class => 'btn primary' %>
  </div>
<% end %>